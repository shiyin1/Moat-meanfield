Cell[TextData[{
 "We can compute the quark propagator with one-loop correction. Here we only \
consider the correction from Pion.\nSo the loop function can be written as\n\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["h", "2"], 
    RowBox[{
     SubscriptBox["C", "2"], "(", 
     SubscriptBox["N", "f"], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        SubscriptBox["\[ImaginaryI]q", "\[Mu]"]}], 
       SubscriptBox["\[Gamma]", "\[Mu]"]}], "-", 
      SubscriptBox["m", "f"]}], ")"}], 
    RowBox[{
     SubscriptBox["G", "q"], "(", "q", ")"}], 
    RowBox[{
     SubscriptBox["G", "\[Pi]"], "(", 
     RowBox[{"q", "-", "p"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fc41337c-62f7-4ab8-842b-7b9a87a6ae3c"],
 "\nThen we perform the projection\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["Z", "q"], "=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["p", "2"]], 
     FractionBox["1", "4"], 
     RowBox[{"tr", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         OverscriptBox["ip", "\[Rule]"], 
         OverscriptBox["\[Gamma]", "\[Rule]"]}], ")"}], "  ", 
       SuperscriptBox["h", "2"], 
       RowBox[{
        SubscriptBox["C", "2"], "(", 
        SubscriptBox["N", "f"], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SubscriptBox["\[ImaginaryI]q", "\[Mu]"]}], 
          SubscriptBox["\[Gamma]", "\[Mu]"]}], "-", 
         SubscriptBox["m", "f"]}], ")"}], 
       RowBox[{
        SubscriptBox["G", "q"], "(", "q", ")"}], 
       RowBox[{
        SubscriptBox["G", "\[Pi]"], "(", 
        RowBox[{"q", "-", "p"}], ")"}]}], "]"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["p", "2"]], 
     OverscriptBox["p", "\[Rule]"], 
     OverscriptBox["q", "\[Rule]"], "cos\[Theta]", " ", 
     SuperscriptBox["h", "2"], 
     RowBox[{
      SubscriptBox["C", "2"], "(", 
      SubscriptBox["N", "f"], ")"}], 
     RowBox[{
      SubscriptBox["G", "q"], "(", "q", ")"}], 
     RowBox[{
      SubscriptBox["G", "\[Pi]"], "(", 
      RowBox[{"q", "-", "p"}], ")"}]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "d84a7af4-3763-4b22-a744-7f7a6d9305a5"],
 "\nTo compute the wave function renormalization of quark at vanishing \
spatial momentum, we can expand the pion propagator at ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["p", "\[Rule]"], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8d166058-a51c-4b49-b8df-1cdd99580151"],
 "\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", "\[Pi]"], "(", 
     RowBox[{"q", "-", "p"}], ")"}], "=", 
    FractionBox["1", 
     RowBox[{
      SuperscriptBox[
       RowBox[{
        SubscriptBox["Z", "0"], "(", 
        RowBox[{
         SubscriptBox["q", "0"], "-", 
         SubscriptBox["p", "0"]}], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{
        SubscriptBox["Z", "s"], "(", 
        RowBox[{
         OverscriptBox["q", "\[Rule]"], "-", 
         OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "+", 
      SuperscriptBox[
       SubscriptBox["m", "\[Pi]"], "2"]}]]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    FractionBox["1", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["q", "0"], "-", 
         SubscriptBox["p", "0"]}], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{
        SubscriptBox["Z", "s"], "(", 
        RowBox[{
         OverscriptBox["q", "\[Rule]"], "-", 
         OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "+", 
      SuperscriptBox[
       SubscriptBox["m", "\[Pi]"], "2"]}]]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["q", "0"], "-", 
          SubscriptBox["p", "0"]}], ")"}], "2"], "+", 
       RowBox[{
        SubscriptBox["Z", "s"], 
        SuperscriptBox[
         OverscriptBox["q", "\[Rule]"], "2"]}], "+", 
       SuperscriptBox[
        SubscriptBox["m", "\[Pi]"], "2"]}]], "-", 
     FractionBox[
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          OverscriptBox["q", "\[Rule]"], "-", 
          OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "-", 
       SuperscriptBox[
        OverscriptBox["q", "\[Rule]"], "2"]}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["q", "0"], "-", 
            SubscriptBox["p", "0"]}], ")"}], "2"], "+", 
         RowBox[{
          SubscriptBox["Z", "s"], 
          SuperscriptBox[
           OverscriptBox["q", "\[Rule]"], "2"]}], "+", 
         SuperscriptBox[
          SubscriptBox["m", "\[Pi]"], "2"]}], ")"}], "2"]], "+", 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            OverscriptBox["q", "\[Rule]"], "-", 
            OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "-", 
         SuperscriptBox[
          OverscriptBox["q", "\[Rule]"], "2"]}], ")"}], "2"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["q", "0"], "-", 
            SubscriptBox["p", "0"]}], ")"}], "2"], "+", 
         RowBox[{
          SubscriptBox["Z", "s"], 
          SuperscriptBox[
           OverscriptBox["q", "\[Rule]"], "2"]}], "+", 
         SuperscriptBox[
          SubscriptBox["m", "\[Pi]"], "2"]}], ")"}], "3"]]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SubscriptBox["G", "\[Pi]"], "(", 
      RowBox[{
       SubscriptBox["q", "0"], "-", 
       SubscriptBox["p", "0"]}], ")"}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           OverscriptBox["q", "\[Rule]"], "-", 
           OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "-", 
        SuperscriptBox[
         OverscriptBox["q", "\[Rule]"], "2"]}], ")"}], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["G", "\[Pi]"], "2"], "(", 
       RowBox[{
        SubscriptBox["q", "0"], "-", 
        SubscriptBox["p", "0"]}], ")"}]}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            OverscriptBox["q", "\[Rule]"], "-", 
            OverscriptBox["p", "\[Rule]"]}], ")"}], "2"], "-", 
         SuperscriptBox[
          OverscriptBox["q", "\[Rule]"], "2"]}], ")"}], "2"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["G", "\[Pi]"], "3"], "(", 
       RowBox[{
        SubscriptBox["q", "0"], "-", 
        SubscriptBox["p", "0"]}], ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SubscriptBox["G", "\[Pi]"], "(", 
      RowBox[{
       SubscriptBox["q", "0"], "-", 
       SubscriptBox["p", "0"]}], ")"}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         OverscriptBox["p", "\[Rule]"], "2"], "-", 
        RowBox[{"2", 
         OverscriptBox["q", "\[Rule]"], " ", 
         OverscriptBox["p", "\[Rule]"], " ", "cos\[Theta]"}]}], ")"}], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["G", "\[Pi]"], "2"], "(", 
       RowBox[{
        SubscriptBox["q", "0"], "-", 
        SubscriptBox["p", "0"]}], ")"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         OverscriptBox["p", "\[Rule]"], "4"], "+", 
        RowBox[{"4", 
         SuperscriptBox[
          OverscriptBox["q", "\[Rule]"], "2"], " ", 
         SuperscriptBox[
          OverscriptBox["p", "\[Rule]"], "2"], " ", 
         SuperscriptBox["cos", "2"], "\[Theta]"}], "-", 
        RowBox[{"4", 
         OverscriptBox["q", "\[Rule]"], " ", 
         SuperscriptBox[
          OverscriptBox["p", "\[Rule]"], "3"], " ", "cos\[Theta]"}]}], ")"}], 
      
      RowBox[{
       SuperscriptBox[
        SubscriptBox["G", "\[Pi]"], "3"], "(", 
       RowBox[{
        SubscriptBox["q", "0"], "-", 
        SubscriptBox["p", "0"]}], ")"}]}]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "807b3189-6176-4964-a261-e6f42623b2d8"],
 "  Here we set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Z", "0"], "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fc28d31c-0187-4aa8-a68e-7c9f2c288efc"],
 "\nHere we keep the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["p", "\[Rule]"], "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "46182744-b396-49d8-84fd-d521df414836"],
 "terms\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{"2", 
     OverscriptBox["q", "\[Rule]"], " ", 
     OverscriptBox["p", "\[Rule]"], " ", "cos\[Theta]", " ", 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["G", "\[Pi]"], "2"], "(", 
      RowBox[{
       SubscriptBox["q", "0"], "-", 
